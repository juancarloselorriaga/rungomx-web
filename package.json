{
  "name": "rungomx-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --raw \"pnpm watch:i18n\" \"next dev -p 8080\"",
    "dev:no-watch": "next dev -p 8080",
    "build": "pnpm generate:i18n && next build",
    "start": "next start",
    "lint": "eslint",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "type-check": "pnpm generate:i18n && tsc --noEmit",
    "test": "pnpm test:app && pnpm test:db",
    "test:app": "jest --selectProjects client server --no-watchman",
    "test:db": "jest --selectProjects database --runInBand --no-watchman",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test --config=e2e/playwright.config.ts",
    "test:e2e:ui": "playwright test --config=e2e/playwright.config.ts --ui",
    "test:e2e:headed": "playwright test --config=e2e/playwright.config.ts --headed",
    "test:e2e:debug": "playwright test --config=e2e/playwright.config.ts --debug",
    "test:e2e:isolated": "node --import tsx scripts/run-e2e-isolated.ts test:e2e",
    "test:e2e:isolated:ui": "node --import tsx scripts/run-e2e-isolated.ts test:e2e:ui",
    "test:e2e:isolated:headed": "node --import tsx scripts/run-e2e-isolated.ts test:e2e:headed",
    "test:e2e:isolated:debug": "node --import tsx scripts/run-e2e-isolated.ts test:e2e:debug",
    "test:ci": "pnpm lint && pnpm generate:i18n && pnpm type-check && pnpm validate:locales && pnpm test && pnpm test:e2e",
    "test:ci:isolated": "pnpm lint && pnpm generate:i18n && pnpm type-check && pnpm validate:locales && pnpm test && pnpm test:e2e:isolated",
    "shadcn": "pnpm dlx shadcn@latest",
    "validate:locales": "node --import tsx scripts/validate-locales.ts",
    "generate:i18n": "pnpm generate:i18n-types && pnpm generate:i18n-loaders",
    "generate:i18n-types": "node --import tsx scripts/generate-i18n-types.ts",
    "generate:i18n-loaders": "node --import tsx scripts/generate-i18n-loaders.ts",
    "watch:i18n": "node --import tsx scripts/watch-i18n.ts",
    "prepare": "simple-git-hooks"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "commit-msg": "scripts/clean-commit-msg.sh"
  },
  "lint-staged": {
    "messages/**/*.json": [
      "pnpm generate:i18n",
      "pnpm validate:locales"
    ]
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@getbrevo/brevo": "^3.0.1",
    "@neondatabase/serverless": "^1.0.2",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tanstack/react-table": "^8.20.5",
    "@tanstack/react-virtual": "^3.13.18",
    "@types/mapbox-gl": "^3.4.1",
    "@vercel/blob": "^2.0.0",
    "better-auth": "^1.4.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.44.7",
    "libphonenumber-js": "^1.12.31",
    "lodash": "^4.17.21",
    "lucide-react": "^0.554.0",
    "mapbox-gl": "^3.17.0",
    "maplibre-gl": "^5.0.0",
    "nanoid": "^5.1.6",
    "next": "16.0.10",
    "next-intl": "^4.5.3",
    "pg": "^8.16.3",
    "qrcode": "^1.5.4",
    "react": "19.2.1",
    "react-day-picker": "^9.11.3",
    "react-dom": "19.2.1",
    "react-map-gl": "^8.1.0",
    "react-markdown": "^10.1.0",
    "react-phone-number-input": "^3.4.14",
    "recharts": "^3.3.0",
    "remark-gfm": "^4.0.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "ws": "^8.18.3",
    "yet-another-react-lightbox": "^3.28.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@noble/hashes": "^2.0.1",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/ws": "^8.18.1",
    "bcryptjs": "^3.0.3",
    "chokidar": "^4.0.3",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.7",
    "eslint": "^9",
    "eslint-config-next": "16.0.10",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "simple-git-hooks": "^2.13.1",
    "tailwindcss": "^4",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.6",
    "typescript": "^5"
  }
}
